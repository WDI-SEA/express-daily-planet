<% include ../partials/header.ejs %>

<h2>Contact Us</h2>

<form class='form-horizontal'>
	<label for="enterName" class="col-sm-2 control-label">Name:</label>
	<div class="col-sm-10 form-group">
	  <input type='text' class='form-control' id='enterName' placeholder='Your Name Here'>
  </div>
  <label for="enterEmail" class="col-sm-2 control-label">Email:</label>
  <div class="col-sm-10 form-group">
    <input type='email' class='form-control' id='enterEmail' placeholder='Your Email Here'>
  </div>
  <label for="enterMessage" class="col-sm-2 control-label">Name:</label>
  <div class="col-sm-10 form-group">
    <textarea rows='3' class='form-control' id='enterMessage' placeholder="What's on your mind?"></textarea>
  </div>
  <div class="form-group">
    <input type='submit' class="col-sm-2" placeholder="Send it!">
  </div>
</form>

