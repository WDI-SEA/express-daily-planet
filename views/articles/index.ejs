<ul>
<% myArticles.forEach(function(article, index){ %> 
	<li> <a href="/articles/<%= index%>"> <%=article.title %>   ||    <%= article.body %> </a> </li>
	<%}); %>
</ul>
<link rel="stylesheet" href="/../css/main.css">
<link href='https://fonts.googleapis.com/css?family=Bangers' rel='stylesheet' type='text/css'>

<form class="Container" method="post" action="/articles">
	<label for="title">Title: </label>
	<input class="text .col-md-3 offset-3" type="text" name="title">
	<label for="body">Body: </label>
	<input class="form" type="text" name="body">
	<input type="submit" value="submit">
</form>


<script src="https://ajax.googleapis.com/ajax/libs/jquery/2.2.0/jquery.min.js"></script>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js"></script>


