<h2>This is a list of all Articles </h2>

<% articles.forEach(function(article) { %>
    <div class="row">
        <div class="col-xs-12">
            <p>
                Title:
                <a href="/article/<%= article.id %>">
                    <%= article.title %>
                </a>
            </p>
            <p>Body:
                <%= article.body %>
            </p>
        </div>
    </div>
    <hr>
    <% }); %>
